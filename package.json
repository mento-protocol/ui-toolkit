    "date-fns": "^2.30.0",
    "eslint": "^9.20.1",
    "eslint-config-next": "^15.1.7",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "framer-motion": "11.5.4",
    "next-themes": "^0.4.4",
    "pino-pretty": "^13.0.0",
    "postcss": "^8.4.49",
